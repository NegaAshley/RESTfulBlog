<% include partials/header %>
    <h1>Welcome to the Blog!</h1>
    <div>
        <% blogs.forEach(function(blog){ %>
            <div class="thumbnail">
                <h1><%= blog.title %></h1>
                <img src="<%= blog.image %>">
                <p><%= blog.body %></p>
            </div>
        <% }); %>
    </div>
<% include partials/footer %>